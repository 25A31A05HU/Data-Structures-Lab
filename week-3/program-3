#include <iostream>
using namespace std;

int main()
{
    int a[10], n, i, key;

    cout << "Enter number of elements and key value: ";
    cin >> n >> key;

    if (n > 10) {
        cout << "Array size exceeds limit";
        return 0;
    }

    cout << "Enter sorted array elements: ";
    for (i = 0; i < n; i++)
    {
        cin >> a[i];
    }

    int low = 0, high = n - 1, mid;
    bool found = false;

    while (low <= high)
    {
        mid = (low + high) / 2;

        if (a[mid] == key)
        {
            cout << "Key element found at index " << mid << endl;
            found = true;
            break;
        }
        else if (a[mid] < key)
        {
            low = mid + 1;
        }
        else
        {
            high = mid - 1;
        }
    }

    if (!found)
    {
        cout << "Key element not found";
    }

    return 0;
}
