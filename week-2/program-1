#include<iostream>
using namespace std;
int main()
{
    int n,a[10],i;
    cout << " Enter the n value:";
    cin >> n;
    cout << "Enter the array elements";
    for(i=0; i<n; i++)
    {
        cin >>a[i];
    }
    cout << "In Array elements: ";
    for(i=0; i<n; i++)
    {
        cout<<a[i]<<" ";
    }
}
