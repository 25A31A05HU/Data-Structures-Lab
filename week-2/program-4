#include <iostream>
using namespace std;
void swap(int &x,int &y);
int main()
{
    int a,b;
    cout << "Enter a and b values: ";
    cin >> a >> b;
    cout << "Before swaping: ";
    cout << "a = " << a << " b = " << b << endl;
    swap(a,b);
    cout<<"After swaping: ";
    cout << "a = " << a << " b = " << b <<endl;
    return 0;
}
void swap(int &x,int &y)
{
    x=x+y;
    y=x-y;
    x=x-y;
}
